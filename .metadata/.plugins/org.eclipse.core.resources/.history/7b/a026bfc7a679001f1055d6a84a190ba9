import java.util.LinkedList;

public class linkedList_02 {

	public static void main(String[] args) {
		
		LinkedList l1 = new LinkedList();
		
		l1.addFirst(10);
		l1.add("java");
		l1.add(true);
		l1.add(20);
		
		System.out.println(l1);
		
		l1.addFirst(30);
		l1.add(2, 44);
		
		System.out.println(l1);
		
		l1.offer("Devops");
		l1.poll("service");
		
		System.out.println(l1);
	}

}
