
class Alpha<A> {
	
	private A ref;
	
	public Alpha(A ref) {
		
		this.ref = ref;
	}
	
	public void disp() {
		
		System.out.println("The type of A is : " + ref.getClass().getName());
	}
	
	public A getRef() {
		
		return ref;
	}
}

public class generic_02 {

	public static void main(String[] args) {
		
		Alpha<Integer> a = new Alpha<>();

	}

}
