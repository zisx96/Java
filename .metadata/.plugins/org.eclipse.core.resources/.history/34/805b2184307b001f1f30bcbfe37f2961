	public static void main(String[] args) {
		
		// type-saftey
		
		int arr[] = new int[3];
		
		ar[0] = 14;
		ar[1] = 24;
		ar[2] = 34;
		ar[3] = 44;
		
		int data = ar[0];
		
		// type-saftey
		
		
		String[] str =  new String[3];
		
		str[0] = "yash";
