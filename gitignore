# Ignore build output directories
bin/
target/

# Eclipse specific files
.classpath
.project
.settings/
.metadata/

# Maven specific files (if using Maven)
.mvn/
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties

# Gradle specific files (if using Gradle)
.gradle/
build/

# Ignore log files and temporary files
*.log
*.tmp
*.swp
*.DS_Store
*.iml

# Ignore compiled class files
*.class

# Ignore package files
*.jar
*.war
*.ear

# IntelliJ IDEA specific files (in case someone opens the project there)
.idea/
*.iml
*.iws

# JRE or JDK libraries
jre/

# STS (Spring Tool Suite) specific files
.springBeans
.spring/

# Ignore node_modules if using Node.js with Spring Boot
node_modules/

# Ignore application-specific properties
application.properties
application.yml
